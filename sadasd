            window.aipDisplayTag.display("MFN_starve-io_300x250")
        }))
    }, this.ping = function() {
        this.socket.send(CLIENT.PING)
    }, this.check_state = function() {
        3 == this.socket.readyState && (this.timeout_server -= CLIENT.TIMEOUT_SERVER)
    }, this.check_pong = function() {
