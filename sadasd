                            break;
                        case 11:
                            e.decrease_item(r[1], r[2]);
                            break;
                        case 12:
                            e.workbench(r[1]);
                            break;
                        case 13:
                            e.hitten_other(r, t.data);
                            break;
                        case 14:
                            e.mute();
                            break;
                        case 15:
                            e.kill_player(r[1]);
                            break;
                        case 16:
                            e.gauges(r[1], r[2], r[3], r[4], r[5], r[6], r[7]);
                            break;
                        case 17:
