                            break;
                        case 22:
                            e.get_time(r[1]);
                            break;
                        case 23:
                            e.set_cam(t.data);
                            break;
                        case 24:
                            e.accept_build(r[1]);
                            break;
                        case 25:
                            e.killed(r[1], t.data);
                            break;
                        case 26:
                            e.minimap(r);
                            break;
                        case 27:
                            e.fail_restore();
                            break;
                        case 28:
                            e.ghost(r[1]);
                            break;
                        case 29:
                            e.reborn();
