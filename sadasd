            A = world.time ? 1 - world.shade.v : world.shade.v;
            ctx.fillStyle = get_color_transition(a.night[0], a.night[1], a.night[2], a.day[0], a.day[1], a.day[2], A)
        } else ctx.fillStyle = a._color[world.time];
        ctx.fillRect(l, c, u, R)
    }
}

