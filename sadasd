        for (n = 0; n < o.length; n++) {
            s = o[n];
            if (o[n].vehicle !== INV.BABY_DRAGON && o[n].vehicle !== INV.BABY_LAVA && o[n].vehicle !== INV.HAWK && o[n].vehicle !== INV.PLANE && o[n].vehicle !== INV.NIMBUS) {
