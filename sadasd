        for (var i = MINIMAP_UTILS[t], r = 0; r < i.length; r++) {
            var a = i[r];
            if (void 0 !== a.tryLoad) a.tryLoad();
            else if (a.length > 0)
                for (var n = 0; n < a.length; n++) void 0 !== a[n].tryLoad && a[n].tryLoad()
        }
}

