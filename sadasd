            return {
                x: e.x + t,
                y: e.y + i
            }
        },
        move: function(e, t, i) {
            e.x += Math.cos(i) * t, e.y += Math.sin(i) * t
        },
        middle: function(e, t) {
            return Math.floor((e - t) / 2)
        },
        middle_point: function(e, t) {
            return {
                x: (e.x + t.x) / 2,
