    for (var n = 0; n < 10; n++) a.rotate(Math.PI / 5), a.save(), a.translate(65 * e, 0), round_regular_polygon(a, 3, 21 * e, 4 * e), fill_path(a, i[1], i[2], 4 * e), a.restore();
