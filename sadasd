 = 1, SOUND_BUILD[ITEMS.REIDITE_DOOR_SPIKE] = 1, SOUND_BUILD[ITEMS.FURNACE] = 1, SOUND_BUILD[ITEMS.RESURRECTION] = 1, SOUND_BUILD[ITEMS.EMERALD_MACHINE] = 1, SOUND_BUILD[ITEMS.CHEST] = 0, SOUND_BUILD[ITEMS.BREAD_OVEN] = 1;
