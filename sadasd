function fun33_token(e, t, i, r, a, n) {
    const o = 8102 ^ i,
        s = (24 << (r >> 16 & 255)) + (32 << (r >> 32 & 255)) + (8 << (r >> 24 & 255)) + (16 << (r >> 8 & 255)),
        _ = 29687 & ~(29687 & a) | ~(29687 & a) & a,
