var DIE = [];
$(".md-languages").on("click", (function() {
    $(this).toggleClass("active")
})), $(".md-languages ul li").on("click", (function() {
    var e = $(this).text();
    "English" === e ? set_english() : "Franais" === e ? set_french() : "Deutsch" === e ? set_german() : "Espaol" === e ? set_spanish() : "Portugus" === e ? set_portuguese() : "" === e ? set_russian() : "Polski" === e && set_polish(), INFO_BOX = [], user.craft.label = [], $(".md-languages ul li").removeClass("active"), $(this).addClass("active"), $(".md-languages label button").text(e)
}));
lang = "EN";
var next_lang, userlang = navigator.language || navigator.userLanguage;
switch (userlang) {
    case "ru":
        lang = "RU";
        break;
    case "en":
        lang = "EN";
        break;
    case "es":
        lang = "SP";
        break;
    case "fr":
        lang = "FR";
        break;
    case "pl":
        lang = "PL";
        break;
    case "de":
        lang = "DE";
        break;
    case "pt":
        lang = "PT"
}
for (var i in "FR" === lang ? ($(".md-languages label button").text("Franais"), next_lang = set_french) : "PL" === lang ? ($(".md-languages label button").text("Polski"), next_lang = set_polish) : "PT" === lang ? ($(".md-languages label button").text("Portugus"), next_lang = set_portuguese) : "SP" === lang ? ($(".md-languages label button").text("Espaol"), next_lang = set_spanish) : "RU" === lang ? ($(".md-languages label button").text(""), next_lang = set_russian) : "DE" === lang ? ($(".md-languages label button").text("Deutsch"), next_lang = set_german) : ($(".md-languages label button").text("English"), next_lang = set_english), IMAGES = {
        LOGO: "img/logo.png",
