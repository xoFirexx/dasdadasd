            var a = {
                x: Math.floor(e[r] * world.w / 250),
                y: Math.floor(e[r + 1] * world.h / 250)
            };
            t && !i && !user.spectator && Utils.dist(a, t) < 250 ? i += 1 : game.minimap.players.push(a)
        }
    }, this.reborn = function() {
