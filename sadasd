        restore_number: function(e) {
            return e >= 2e4 ? e = 1e3 * (e - 2e4) : e >= 1e4 && (e = 100 * (e - 1e4)), e
        },
        simplify_number: function(e) {
            if ("number" != typeof e) return "0";
            if (e >= 1e4) {
                var t = Math.floor(Math.log10(e)) - 2,
                    i = Math.max(0, 3 - t),
