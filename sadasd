function create_hurt_player(e, t) {
    var i = document.createElement("canvas"),
        r = i.getContext("2d"),
        a = 120 * e,
        n = 110 * e,
        o = a / 2,
        s = n / 2,
        _ = 20 * e,
        T = 112 * e,
        E = 82 * e,
        I = T / 2,
        l = E / 2,
        u = 4 * e;
    return i.width = a, i.height = n, r.globalAlpha = 1, r.translate(o, s), round_rect(r, -I, -l, T, E, _), fill_path(r, t, t, u), i
}

